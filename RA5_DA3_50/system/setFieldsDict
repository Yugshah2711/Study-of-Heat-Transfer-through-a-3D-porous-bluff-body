FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
		volScalarFieldValue			porosityIndex 0
);

regions
(
	zoneToCell
	{
		name					"porousZone";
		fieldValues
		(
			volScalarFieldValue		porosityIndex 1
		);
	}
);



